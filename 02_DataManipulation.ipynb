   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "df_all.drop(['tq_db26_1','tq_db26_2','tq_db33_13','tq_db34_13','tq_db44_1','tq_db44_2','tq_db44_3'], axis=1, inplace=True)"
